    "@graph": [
        {
            "@id": "uncl4431:1",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "Payment of an invoice is made by a factor under the guarantee he issued to seller or to another factor.",
            "rdfs:label": "Factor guarantee",
            "rdf:value": "1"
        },
        {
            "@id": "uncl4431:10",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "A bank has agreed to stand as guarantor to ensure that payment is made.",
            "rdfs:label": "Bank guarantee",
            "rdf:value": "10"
        },
        {
            "@id": "uncl4431:11",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "A public authority has agreed to stand as guarantor to ensure that payment is made.",
            "rdfs:label": "Public authority guarantee",
            "rdf:value": "11"
        },
        {
            "@id": "uncl4431:12",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "The party who has agreed to stand as guarantor to ensure that payment is made is neither the payee nor the payer.",
            "rdfs:label": "Third party guarantee",
            "rdf:value": "12"
        },
        {
            "@id": "uncl4431:13",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "The guarantee of payment is in the form of a standby letter of credit.",
            "rdfs:label": "Standby letter of credit",
            "rdf:value": "13"
        },
        {
            "@id": "uncl4431:14",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "No guarantee of payment has been made or is available.",
            "rdfs:label": "No guarantee",
            "rdf:value": "14"
        },
        {
            "@id": "uncl4431:20",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "The payer has provided possession of, or title in goods, as security against payment.",
            "rdfs:label": "Goods as security",
            "rdf:value": "20"
        },
        {
            "@id": "uncl4431:21",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "The payer has provided title in, or a lien over a business whose assets may be sold or sequestered, as security against payment.",
            "rdfs:label": "Business as security",
            "rdf:value": "21"
        },
        {
            "@id": "uncl4431:23",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "The payer has provided a warrant or warehouse receipts for goods or property to be held or used as security against payment.",
            "rdfs:label": "Warrant or similar (warehouse receipts)",
            "rdf:value": "23"
        },
        {
            "@id": "uncl4431:24",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "The payer has provided a mortgage as security against payment.",
            "rdfs:label": "Mortgage",
            "rdf:value": "24"
        },
        {
            "@id": "uncl4431:45",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "A certificate of insurance has been provided as a guarantee of eventual payment.",
            "rdfs:label": "Insurance certificate",
            "rdf:value": "45"
        },
        {
            "@id": "uncl4431:ZZZ",
            "@type": "unece:UNCL4431Code",
            "rdfs:comment": "A code assigned within a code list to be used on an interim basis and as defined among trading partners until a precise code can be assigned to the code list.",
            "rdfs:label": "Mutually defined",
            "rdf:value": "ZZZ"
        }
    ]
}