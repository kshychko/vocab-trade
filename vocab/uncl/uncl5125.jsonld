    "@graph": [
        {
            "@id": "uncl5125:AAA",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "The price stated is the net price including allowances/ charges. Allowances/charges may be stated for information only.",
            "rdfs:label": "Calculation net",
            "rdf:value": "AAA"
        },
        {
            "@id": "uncl5125:AAB",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "The price stated is the gross price to which allowances/ charges must be applied.",
            "rdfs:label": "Calculation gross",
            "rdf:value": "AAB"
        },
        {
            "@id": "uncl5125:AAC",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "The price does not include the allowances and charges, but includes the taxes.",
            "rdfs:label": "Allowances and charges not included, tax included",
            "rdf:value": "AAC"
        },
        {
            "@id": "uncl5125:AAD",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "Average selling price of a product.",
            "rdfs:label": "Average selling price",
            "rdf:value": "AAD"
        },
        {
            "@id": "uncl5125:AAE",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "The price stated is for information purposes only and excludes all allowances and charges. Taxes however are included in the price.",
            "rdfs:label": "Information price, excluding allowances or charges, including taxes",
            "rdf:value": "AAE"
        },
        {
            "@id": "uncl5125:AAF",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "The price stated is for information purposes only and excludes all allowances, charges and taxes.",
            "rdfs:label": "Information price, excluding allowances or charges, and taxes",
            "rdf:value": "AAF"
        },
        {
            "@id": "uncl5125:AAG",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "A code to indicate that the price described is an additive component of the total price.",
            "rdfs:label": "Additive unit price component",
            "rdf:value": "AAG"
        },
        {
            "@id": "uncl5125:AAH",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "Average purchase price of a product.",
            "rdfs:label": "Average purchase price",
            "rdf:value": "AAH"
        },
        {
            "@id": "uncl5125:CAL",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "The price stated is the price for the calculation of the line item amount.",
            "rdfs:label": "Calculation price",
            "rdf:value": "CAL"
        },
        {
            "@id": "uncl5125:INF",
            "@type": "unece:UNCL5125Code",
