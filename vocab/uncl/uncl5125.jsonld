            "rdfs:label": "Information",
            "rdf:value": "INF"
        },
        {
            "@id": "uncl5125:INV",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "Referenced price taken from an invoice.",
            "rdfs:label": "Invoice price",
            "rdf:value": "INV"
        },
        {
            "@id": "uncl5125:ZZZ",
            "@type": "unece:UNCL5125Code",
            "rdfs:comment": "A code assigned within a code list to be used on an interim basis and as defined among trading partners until a precise code can be assigned to the code list.",
            "rdfs:label": "Mutually defined",
            "rdf:value": "ZZZ"
